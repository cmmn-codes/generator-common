{
<% if (extending) { -%>
  "extends": "<%- extending %>",
<%_ } -%>
  "compilerOptions": {
<% if (jsx) { -%>
    "jsx": "<%- jsx %>",
<%_ } -%>
    "target": "<%- target %>",
    "rootDir": "<%- rootDir %>",
    "baseUrl": "<%- baseUrl %>",
    "declarations": <%- declarations %>,
    "composite": <%- composite %>,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "moduleResolution": "<%- moduleResolution %>",
    "module": "<%- module %>",
    "outDir": "<%- outDir %>"
  },
  "include": [
<% includes.forEach((inc, i) => { -%>
    "<%- inc %>"<%- i < includes.length - 1 ? ',' : '' %>
<%_ }); %><%_ _%>
  ],
  "exclude": [
<% excludes.forEach((exc, i) => { -%>
     "<%- exc %>"<%- i < excludes.length-1 ? ',' : '' %>
<%_ }); %><%_ _%>
  ]
}
